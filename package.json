{
  "name": "react-redux-template",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:yoshiko-pg/react-redux-template.git",
  "author": "maasa <maasa.giss2@gmail.com>",
  "license": "MIT",
  "scripts": {
    "setup": "yarn install && yarn flow-typed install",
    "build": "parcel src/index.html",
    "format": "prettier --single-quote --trailing-comma=all \"**/*.js\"",
    "format:check": "npm run format -- --list-different",
    "format:fix": "npm run format -- --write",
    "lint": "eslint --ext .js --ext .jsx src ./",
    "lint:fix": "npm run lint -- --fix",
    "typecheck": "flow check",
    "check": "npm run format:check && npm run lint && npm run typecheck && npm run test",
    "check:fix": "npm run format:fix && npm run lint:fix && npm run typecheck && npm run test",
    "precommit": "lint-staged",
    "prepush": "npm run check"
  },
  "pre-commit": [
    "check:fix"
  ],
  "dependencies": {
    "react": "^16.2.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "ress": "^1.2.2"
  },
  "devDependencies": {
    "babel-eslint": "^8.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.14.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-flowtype": "^2.40.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-prettier": "^2.4.0",
    "eslint-plugin-react": "^7.5.1",
    "flow-bin": "^0.61.0",
    "flow-typed": "^2.2.3",
    "lint-staged": "^6.0.0",
    "parcel-bundler": "^1.3.1",
    "pre-commit": "^1.2.2",
    "prettier": "1.9.2"
  }
}
